echo hello world
ls -l -a
echo "hello world"
echo 'single quotes'
echo "mixed 'quotes'"
echo 'nested "quotes" inside'
echo hello\ world
echo "escaped space: \"hello world\""
echo 'escaped single quote: '\''hello'\'''
ls -l | grep minishell
cat file.txt > output.txt
cat < input.txt | grep "pattern" > output.txt
echo $HOME
echo "$USER's home is $HOME"
ls -l; echo "done"; pwd
sleep 5 &
ls -l & echo "background process"
(echo hello; ls)
echo "unterminated string
echo 'another unterminated string
echo "" | cat -e
echo "a    b" | tr -s ' '
